{
    "name": "Browser Wallet",
    "manifest_version": 2,
    "version": "1.0",
    "browser_action": {
        "default_title": "Browser Wallet",
        "default_popup": "index.html"
    },

    "content_scripts": [
        {
          "matches": ["http://*/*", "https://*/*"],
          "css": ["style.css"]
        }
    ]
}