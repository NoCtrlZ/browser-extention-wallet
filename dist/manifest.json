{
    "name": "Browser Wallet",
    "manifest_version": 2,
    "version": "1.0",
    "description": "This is browser wallet for blockchain",
    "browser_action": {
        "default_icon": "16.png",
        "default_title": "Browser Wallet",
        "default_popup": "index.html"
    },

    "background": {
        "scripts": ["js/events.js"],
        "persistent": false
